{
  "name": "@aliothor/cesium-martini",
  "version": "1.1.0",
  "description": "Create cesium terrain through raster tile service",
  "keywords": [
    "cesium",
    "martini",
    "terrain",
    "mapbox"
  ],
  "repository": {
    "url": "https://github.com/aliothor/cesium-martini"
  },
  "license": "MIT",
  "author": "aliothor",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "prepack": "pnpm run build",
    "release": "changelogen --release && pnpm publish && git push --follow-tags",
    "test": "pnpm test:types && vitest run",
    "test:types": "tsc --noEmit --skipLibCheck"
  },
  "dependencies": {
    "ndarray": "^1.0.19"
  },
  "devDependencies": {
    "@types/ndarray": "^1.0.14",
    "@types/node": "^20.12.10",
    "changelogen": "^0.5.5",
    "typescript": "^5.4.5",
    "unbuild": "^2.0.0",
    "vite": "^5.2.11",
    "vite-plugin-dts": "^3.9.1",
    "vitest": "^1.6.0"
  },
  "peerDependencies": {
    "cesium": "^1.115.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
